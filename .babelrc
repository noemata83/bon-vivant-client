{
  "presets": ["@babel/preset-typescript", "next/babel"],
  "plugins": [
    ["@babel/plugin-transform-typescript",
    {
      "allowDeclareFields": true
    }
    ],
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "@babel/plugin-proposal-decorators", { "legacy" : true }
    ],
    "import-graphql"
  ]
}
